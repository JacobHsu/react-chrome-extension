{
  "short_name": "My Bookmarks",
  "name": "My Bookmarks",
  "version": "1.1",
  "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon32.png",
      "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'sha256-0nQXp4nN/Rx8lxl6g8sX/josZokWwdpveLhHKUaubSI=' https://ajax.googleapis.com; object-src 'self'"
}
